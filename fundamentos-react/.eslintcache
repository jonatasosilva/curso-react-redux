t React from \"react\";\n\nexport default (props) => {\n\n  return (\n    <div>\n      {props.nome} <strong>{props.sobrenome}</strong>\n    </div>\n  );\n};","/home/jonatas/curso-react-redux/fundamentos-react/src/components/repeticao/ListaAlunos.jsx",["132"],"import React from \"react\";\nimport alunos from \"../../data/alunos\";\n\nexport default props => {\n  const alunosLI = alunos.map(aluno => {\n    return (\n      <li key={aluno.id}>\n        {aluno.id}) {aluno.nome} - {aluno.nota}\n      </li>\n    );\n  });\n\n  return (\n    <div>\n      <ul style={{ listStyle: \"none\" }}>{alunosLI}</ul>\n    </div>\n  );\n};","/home/jonatas/curso-react-redux/fundamentos-react/src/data/produtos.js",[],"/home/jonatas/curso-react-redux/fundamentos-react/src/components/repeticao/TabelaProdutos.jsx",["133"],"import \"./TabelaProdutos.css\";\nimport React from \"react\";\nimport produtos from \"../../data/produtos\";\n\nexport default props => {\n\n  function getLinhas() {\n    return produtos.map((produto, i) => {\n      return (\n        <tr key={produto.id}\n          className={i % 2 === 0 ? 'Par': 'Impar'}>\n          <td>{produto.id}</td>\n          <td>{produto.nome}</td>\n          <td>R$ {produto.preco.toFixed(2).replace('.', ',')}</td>\n        </tr>\n      );\n    });\n  }\n\n  return (\n    <div className=\"TabelaProdutos\">\n      <table>\n        <thead>\n          <tr>\n            <th>Id</th>\n            <th>Nome</th>\n            <th>Preço</th>\n          </tr>\n        </thead>\n        <tbody>\n          {getLinhas()}\n        </tbody>\n      </table>\n    </div>\n  );\n};","/home/jonatas/curso-react-redux/fundamentos-react/src/components/condicional/ParOuImpar.jsx",["134"],"import React from \"react\";\n\nexport default props => {\n  const isPar = props.numero % 2 === 0;\n  return <div>{ isPar ? <span>Par</span> : <span>Ímpar</span>}</div>\n};","/home/jonatas/curso-react-redux/fundamentos-react/src/components/condicional/UsuarioInfo.jsx",["135"],"import React from \"react\";\nimport If, { Else } from \"./If\";\n\nexport default props => {\n  const usuario = props.usuario || {}\n  return (\n    <div>\n      <If test={usuario && usuario.nome}>\n        Seja bem vindo <strong>{ usuario.nome }</strong>!\n        <Else>\n          Seja bem vindo <strong>Amigão</strong>!\n        </Else>\n      </If>\n    </div>\n  );\n};","/home/jonatas/curso-react-redux/fundamentos-react/src/components/condicional/If.js",["136"],"export default props => {\n  const elseChild = props.children.filter(child => {\n    return child.type && child.type.name === 'Else';\n  })[0]\n\n  const ifChildren = props.children.filter(child => {\n    return child !== elseChild;\n  }) \n\n  if(props.test) {\n    return ifChildren;\n  } else if (elseChild) {\n    return elseChild;\n  } else {\n    return false;\n  }\n};\n\nexport const Else = props => props.children;","/home/jonatas/curso-react-redux/fundamentos-react/src/data/alunos.js",["137"],"export default [\n  { id: 1, nome: 'Ana', nota: 6.2 },\n  { id: 2, nome: 'Bia', nota: 7.6 },\n  { id: 3, nome: 'Carlos', nota: 8.1 },\n  { id: 4, nome: 'Daniel', nota: 5.7 },\n  { id: 5, nome: 'Gui', nota: 10.0 },\n  { id: 6, nome: 'Rebeca', nota: 9.5 },\n  { id: 7, nome: 'Arthur', nota: 7.7 },\n  { id: 8, nome: 'Pedro', nota: 6.9 },\n  { id: 9, nome: 'Gustavo', nota: 8.1 },\n]","/home/jonatas/curso-react-redux/fundamentos-react/src/components/comunicao/DiretaPai.jsx",["138"],"import React from \"react\";\nimport DiretaFilho from \"./DiretaFilho\";\n\nexport default props => {\n\n\treturn (\n\t\t<div>\n\t\t\t<DiretaFilho nome=\"Junior\" idade={20} nerd={true} />\n\t\t\t<DiretaFilho nome=\"Gabriel\" idade={17} nerd={false} />\n\t\t</div>\n\t);\n};","/home/jonatas/curso-react-redux/fundamentos-react/src/components/comunicao/DiretaFilho.jsx",["139"],"import React from \"react\";\n\nexport default props => {\n\n\treturn (\n\t\t<div>\n\t\t\t<span>{props.nome} </span>\n\t\t\t<span><strong>{props.idade}</strong> </span>\n\t\